{
  "name": "mag-react-starter-kit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "bootstrap": "^5.1.3",
    "env-cmd": "^10.1.0",
    "react": "^17.0.2",
    "react-bootstrap": "^2.3.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "react-redux-loading-bar": "^5.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "4.0.3",
    "redux-thunk": "^2.4.1",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "env-cmd -f src/environments/.env.local react-scripts start",
    "start:dev": "env-cmd -f src/environments/.env.development react-scripts start",
    "start:prod": "env-cmd -f src/environments/.env.production react-scripts start",
    "build:dev": "env-cmd -f src/environments/.env.development react-scripts build",
    "build:prod": "env-cmd -f src/environments/.env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{js,css,scss,html}\"",
    "rtl": "webpack"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "redux-devtools-extension": "^2.13.9"
  }
}
